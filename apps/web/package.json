    "isbot": "^5.1.32",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "catalog:tailwind",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@yukine/tsconfig": "workspace:*",
    "alchemy": "catalog:dev",
