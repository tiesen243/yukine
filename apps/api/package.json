{
  "name": "@yukine/api",
  "version": "0.0.1",
  "private": true,
  "description": "Yukine API server powered by Elysia and Effect",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./server": "./dist/server.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bun run --hot --env-file=../../.env src/server.ts",
    "start": "bun run --env-file=../../.env ./dist/server.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.1",
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/openapi": "^1.4.13",
    "@yukine/validators": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.14",
    "elysia": "^1.4.22"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@yukine/tsconfig": "workspace:*",
    "drizzle-kit": "^0.31.8",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
